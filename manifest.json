{
    "name" : "Surfcave",
    "version" : "0.1.0",
    "description" : "Counterpart to Surfcave. Tracking all images from your browser.",
    "permissions" : [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts" : [
        {
            "matches" : ["http://*/*", "https://*/*"], 
            "js" : ["grab.js"]
        }
    ],
    "background_page" : "background.html",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "images/icon.png"
    }
}
